package com.company;

public class Arrays {

    public static void main(String[] args) {

        int[] numbers = new int[12];
        for (int i = 0; i < 12; i++) {
            numbers[i] = 1;
            System.out.println(numbers[i]);
        }

        int[] array1 = {2, 4, 6, 8, 10};

        for (int i = 0; i < 5; i++) {
            System.out.println(array1[i]);
        }

        for (int i = 0; i < 5 / 2; i++) {
            int temp = array1[i];
            array1[i] = array1[5 - 1 - i];
            array1[5 - i - 1] = temp;
        }

        for (int i = 0; i < 5; i++) {
            System.out.println(array1[i]);
        }
    }

/*
        int[] array2 = {2, 4, 6, 8, 10};
        int[] array3 = new int[5];

        for (int j = 0; j < 5; j++)

        */


}

